@model FullStackRestaurantMVC.Models.ViewModels.TableViewModel

@{
    ViewData["Title"] = "Edit Table";
}

<h2>Edit Table</h2>

<form asp-action="EditTable" method="post">
    <input type="hidden" asp-for="Id" />
    <partial name="_TableForm" model="Model" />
    <button type="submit" class="btn btn-primary">Save</button>
    <a asp-action="Tables" class="btn btn-secondary">Cancel</a>
</form>
